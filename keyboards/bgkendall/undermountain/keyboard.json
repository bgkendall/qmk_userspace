{
    "keyboard_name": "Undermountain",
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu",
    "diode_direction": "COL2ROW",
    "build": {
        "lto": true
    },
    "features": {
        "audio": false,
        "backlight": true,
        "bluetooth": false,
        "bootmagic": false,
        "bootmagic_lite": true,
        "command": false,
        "console": false,
        "deferred_exec": true,
        "encoder": true,
        "encoder_map": true,
        "extrakey": true,
        "magic": false,
        "midi": false,
        "mousekey": false,
        "music": false,
        "nkro": false,
        "os_detection": true,
        "rgblight": false,
        "unicode": false
    },
    "matrix_pins": {
        "direct": [
            ["B3", "F6"],
            ["D4", "E6"]
        ]
    },
    "backlight": {
        "pin": "B6",
        "default": {
            "on": false,
            "brightness": 4
        },
        "levels": 8,
        "breathing": true
    },
    "indicators": {
        "caps_lock": "B2",
        "on_state": 0
    },
    "encoder": {
        "rotary": [
            { "pin_a": "B1", "pin_b": "F7" }
            { "pin_a": "F5", "pin_b": "F4" },
        ]
    },
    "usb": {
        "pid": "0x554D",
        "device_version": "1.0.0",
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [1, 0], "y":1.5, "x":0},
                {"matrix": [1, 1], "y":1.5, "x":1},
                {"matrix": [0, 0], "y":0,   "x":0},
                {"matrix": [0, 1], "y":0,   "x":1}
            ]
        }
    }
}
